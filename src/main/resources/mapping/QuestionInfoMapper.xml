<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.videoStudy.data.dao.QuestionInfoMapper">
    <select id="selectAll" resultType="com.videoStudy.data.model.QuestionInfo">
        SELECT * FROM QuestionInfo;
    </select>
    <select id="selectById" resultType="com.videoStudy.data.model.QuestionInfo">
        SELECT * FROM QuestionInfo WHERE id = #{id};
    </select>
</mapper>